              rdfs:domain <http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLEWL#Equipment> ;
              rdfs:range :Parameters .


###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLEWL#Has
<http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLEWL#Has> rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLE#hasFIBHighTension
:hasFIBHighTension rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasHighTension ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:minInclusive 0.2
                                                       ]
                                                       [ xsd:maxInclusive 30
                                                       ]
                                                     )
                              ] .


###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLE#hasFIBmagnification
:hasFIBmagnification rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :hasMagnification .


###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLE#hasHighTension
:hasHighTension rdf:type owl:DatatypeProperty ;
                rdfs:domain [ rdf:type owl:Restriction ;
                              owl:onProperty :has-unit ;
                              owl:someValuesFrom <http://purl.obolibrary.org/obo/uo#kilovolt>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasParameter ;
                              owl:someValuesFrom :HighTension
                            ] ;
                rdfs:range xsd:integer .


###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLE#hasMagnification
:hasMagnification rdf:type owl:DatatypeProperty ;
                  rdfs:domain [ rdf:type owl:Restriction ;
                                owl:onProperty :hasParameter ;
                                owl:someValuesFrom :Magnification
                              ] ;
                  rdfs:range rdfs:Literal .


###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLE#hasSEMHighTension
:hasSEMHighTension rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :hasHighTension ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:minInclusive 0.2
                                                       ]
                                                       [ xsd:maxInclusive 30
                                                       ]
                                                     )
                              ] .


###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLE#hasSEMmagnification
:hasSEMmagnification rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf :hasMagnification .


###  http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLEWL#hasSpeed
<http://www.semanticweb.org/hr7456/ontologies/2021/8/MSLEWL#hasSpeed> rdf:type owl:DatatypeProperty .


