    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_lm_atomic_multipoles ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim2 ],
        :NMI_quantity .

:atomic_numbers_primitive a owl:Class ;
    rdfs:comment "Atomic numbers in the primitive cell."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms_primitive ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:atomic_numbers_std a owl:Class ;
    rdfs:comment "Atomic numbers of the atoms in the standardized cell."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms_std ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:band_energies a owl:Class ;
    rdfs:comment """$k$-dependent or $q$-dependent  energies of the electronic or vibrational band
segment (electronic/vibrational band structure). This is a third-order tensor,
with one dimension used for the spin channels (1 in case of a vibrational band
structure), one for the $k$ or $q$ points for each segment, and one for the
eigenvalue sequence."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_band_segment_eigenvalues ;
            owl:onProperty :hasVarDim3 ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_k_points_per_segment ;
            owl:onProperty :hasVarDim2 ],
        :NMI_quantity .

:band_energies_normalized a owl:Class ;
    rdfs:comment """$k$-dependent energies of the electronic band segment (electronic band structure)
with respect to the top of the valence band. This is a third-order tensor, with
one dimension used for the spin channels, one for the $k$ points for each segment,
and one for the eigenvalue sequence."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_normalized_k_points_per_segment ;
