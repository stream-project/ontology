@prefix : <https://nomad-coe.eu/ontology/metainfo/pure#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://nomad-coe.eu/ontology/metainfo/pure> a owl:Ontology .

:basis_set_description a owl:Class ;
    rdfs:comment """One of the parts building the basis set of the system (e.g., some atom-centered basis
set, plane-waves or both)."""^^xsd:string ;
    rdfs:subClassOf :NMI_category .

:conserved_quantity a owl:Class ;
    rdfs:comment """A quantity that is preserved during the time propagation (for example,
kinetic+potential energy during NVE)."""^^xsd:string ;
    rdfs:subClassOf :NMI_category .

:error_estimate a owl:Class ;
    rdfs:comment "An estimate of the error on the converged (final) value."^^xsd:string ;
    rdfs:subClassOf :error_estimate_contribution .

:hasFixDim5 a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain :NMI_quantity ;
    rdfs:range [ a rdfs:Datatype ;
            owl:onDatatype xsd:integer ;
            owl:withRestrictions ( [ xsd:minInclusive 1 ] ) ] .

:hasReference a owl:ObjectProperty ;
    rdfs:domain :NMI_quantity ;
    rdfs:range :NMI_section ;
    rdfs:subPropertyOf <http://emmo.info/emmo/middle/holistic#EMMO_ae2d1a96_bfa1_409a_a7d2_03d69e8a125a> .

:hasSection a owl:ObjectProperty ;
    rdfs:domain :NOMADEntry ;
    rdfs:range :NMI_section .

:hasVarDim5 a owl:ObjectProperty ;
    rdfs:domain :NMI_quantity ;
    rdfs:range :NMI_dimension ;
    rdfs:subPropertyOf <http://emmo.info/emmo/middle/semiotics#EMMO_eb3518bf_f799_4f9e_8c3e_ce59af11453b> .

:section_run a owl:Class ;
    rdfs:comment """Every section_run represents a single call of a program. What exactly is contained in a run depends on the run type (see for example section_method and
section_single_configuration_calculation) and the program (see [program_info
](program_info))."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :parsing_message_debug_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :parsing_message_warning_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :time_run_wall_end ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :parsing_message_error_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :run_clean_end ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_system ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_basis_set_cell_dependent ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :message_error_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :program_compilation_host ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :time_run_wall_start ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_basis_set_atom_centered ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :program_basis_set_type ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :message_info_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_method ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_topology ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :program_version ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_sampling_method ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_single_configuration_calculation ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :time_run_date_start ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :raw_id ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :parsing_message_info_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :program_name ],
        [ a owl:Restriction ;
            owl:onProperty :hasSubSection ;
            owl:someValuesFrom :section_frame_sequence ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :time_run_cpu1_start ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :run_hosts ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :program_compilation_datetime ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :time_run_cpu1_end ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :message_warning_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :message_debug_run ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :calculation_file_uri ],
        [ a owl:Restriction ;
            owl:onProperty :hasQuantity ;
            owl:someValuesFrom :time_run_date_end ],
        :NMI_section .

:settings_GW a owl:Class ;
    rdfs:comment """Contains parameters for the GW-method in the post Hartree-Fock step, that expands the
self-energy in terms of the single particle Green's function $G$ and the screened
Coulomb interaction $W$."""^^xsd:string ;
    rdfs:subClassOf :settings_post_hartree_fock .

:settings_MCSCF a owl:Class ;
    rdfs:comment """Contains parameters for the multi-configurational self-consistent-field (MCSCF)
method."""^^xsd:string ;
    rdfs:subClassOf :settings_post_hartree_fock .

:settings_Monte_Carlo a owl:Class ;
    rdfs:comment "Contains parameters that control the Monte-Carlo sampling."^^xsd:string ;
    rdfs:subClassOf :settings_sampling .

:settings_barostat a owl:Class ;
    rdfs:comment "Contains parameters controlling the barostat in a molecular dynamics calculation."^^xsd:string ;
    rdfs:subClassOf :settings_molecular_dynamics .

:settings_constraint a owl:Class ;
    rdfs:comment "Some parameters that describe a constraint"^^xsd:string ;
    rdfs:subClassOf :NMI_category .

:settings_coupled_cluster a owl:Class ;
    rdfs:comment "Contains parameters for the coupled-cluster method (CC) in the post Hartree-Fock step."^^xsd:string ;
    rdfs:subClassOf :settings_post_hartree_fock .

:settings_integrator a owl:Class ;
    rdfs:comment "Contains parameters that control the molecular dynamics (MD) integrator."^^xsd:string ;
    rdfs:subClassOf :settings_molecular_dynamics .

:settings_interaction a owl:Class ;
    rdfs:comment "Some parameters that describe a bonded interaction."^^xsd:string ;
    rdfs:subClassOf :NMI_category .

:settings_metadynamics a owl:Class ;
    rdfs:comment "Contains parameters that control the metadynamics sampling."^^xsd:string ;
    rdfs:subClassOf :settings_sampling .

:settings_moller_plesset_perturbation_theory a owl:Class ;
    rdfs:comment "Contains parameters for Møller–Plesset perturbation theory."^^xsd:string ;
    rdfs:subClassOf :settings_post_hartree_fock .

:settings_multi_reference a owl:Class ;
    rdfs:comment """Contains parameters for the multi-reference single and double configuration
interaction method."""^^xsd:string ;
    rdfs:subClassOf :settings_post_hartree_fock .

:settings_run a owl:Class ;
    rdfs:comment """Contains parameters that control the whole run (but not the *single configuration
calculation*, see section_single_configuration_calculation)."""^^xsd:string ;
    rdfs:subClassOf :NMI_category .

:settings_thermostat a owl:Class ;
    rdfs:comment """Contains parameters that control the thermostat in the molecular dynamics (MD)
calculations."""^^xsd:string ;
    rdfs:subClassOf :settings_molecular_dynamics .

:NOMADEntry a owl:Class ;
    rdfs:subClassOf owl:Thing .

:SC_matrix a owl:Class ;
    rdfs:comment """Specifies the matrix that transforms the unit-cell into the super-cell in which
the actual calculation is performed."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim1 ],
        :NMI_quantity .

:XC_functional a owl:Class ;
    rdfs:comment """This value describes a DFT exchange-correlation (XC) functional used for
evaluating the energy value stored in energy_XC_functional and related quantities
(e.g., forces).

It is a unique short name obtained by combining the data stored in
section_XC_functionals, more specifically by combining different
XC_functional_name as described in the [XC_functional wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/XC-
functional)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_XC ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_XC_functional ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_physical_parameter ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_potential_energy_surface ],
        :NMI_quantity .

:XC_functional_name a owl:Class ;
    rdfs:comment """Provides the name of one of the exchange and/or correlation (XC) functionals
combined in XC_functional.

The valid unique names that can be used are listed in the [XC_functional wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/XC-
functional).

*NOTE*: This value should refer to a correlation, an exchange or an exchange-
correlation functional only."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_physical_parameter ],
        :NMI_quantity .

:XC_functional_parameters a owl:Class ;
    rdfs:comment """Contains an associative list of non-default values of the parameters for the
functional declared in XC_functional_name of the section_XC_functionals section.

For example, if a calculations using a hybrid XC functional (e.g., HSE06)
specifies a user-given value of the mixing parameter between exact and GGA
exchange, then this non-default value is stored in this metadata.

The labels and units of these values are defined in the paragraph dedicated to the
specified functional declared in XC_functional_name of the [XC_functional wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/XC-
functional).

If this metadata is not given, the default parameter values for the
XC_functional_name are assumed."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_physical_parameter ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:XC_functional_weight a owl:Class ;
    rdfs:comment """Provides the value of the weight for the exchange, correlation, or exchange-
correlation functional declared in XC_functional_name (see
section_XC_functionals).

This weight is used in the linear combination of the different XC functional names
(XC_functional_name) in different section_XC_functionals sections to form the
XC_functional used for evaluating energy_XC_functional and related quantities.

If not specified then the default is set to 1."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_physical_parameter ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:XC_method a owl:Class ;
    rdfs:comment """Describes the exchange correlation (XC) method used for evaluating the XC energy
(energy_XC). Differently from XC_functional, perturbative treatments are also
accounted for, where the string contains the reference to both the perturbative
(e.g., MP2) and the starting point (e.g, Hartree-Fock) XC method defined in the
section section_method.

The value consists of XC_method_current concatenated with the `@` character and
the XC method (XC_method) defined in section_method that is referred to by
method_to_method_ref where method_to_method_kind = "starting_point_method"."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_potential_energy_surface ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_XC ],
        :NMI_quantity .

:XC_method_current a owl:Class ;
    rdfs:comment """Identifies the exchange correlation (XC) method used for energy_XC and related
quantities in a standardized short form as a string.

It is built by joining the values in the following order using the underscore `_`
character: electronic_structure_method, XC_functional,
self_interaction_correction_method, van_der_Waals_method and relativity_method.

If any of the methods listed in the string contain non-standard settings, then the
first 10 characters of the Base64 URL encoding of SHA 512 checksum of a normalized
JSON with all non-redundant non-derived settings_XC are appended to the the string
preceded by an underscore.

With empty strings, the underscore `_` character is skipped.

If the method defined in the section_method section is perturbative, the
XC_method_current contains only the perturbative method, not the starting point
(e.g. the DFT XC functional used as a starting point for a RPA perturbative
calculation). In this case, the string that contains both the perturbative and
starting point method is stored in XC_method."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_potential_energy_surface ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_XC ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:atom_atom_number a owl:Class ;
    rdfs:comment "Atomic number Z of the atom."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_sites ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:atom_concentrations a owl:Class ;
    rdfs:comment """concentration of the atom species in a variable composition, by default it should
be considered an array of ones. Summing these should give the number_of_sites"""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:atom_forces a owl:Class ;
    rdfs:comment """Forces acting on the atoms, calculated as minus gradient of energy_total,
**including** constraints, if present. The derivatives with respect to
displacements of nuclei are evaluated in Cartesian coordinates. In addition, these
forces are obtained by filtering out the unitary transformations (center-of-mass
translations and rigid rotations for non-periodic systems, see
atom_forces_free_raw for the unfiltered counterpart). Forces due to constraints
such as fixed atoms, distances, angles, dihedrals, etc. are included (see
atom_forces_raw for the unfiltered counterpart)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_a979c531_f9fa_4a6e_93c1.0.0-alpha2960241ca6> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :atom_forces_type ],
        :NMI_quantity .

:atom_forces_T0 a owl:Class ;
    rdfs:comment """Forces acting on the atoms, calculated as minus gradient of energy_total_T0,
**including** constraints, if present. The derivatives with respect to
displacements of the nuclei are evaluated in Cartesian coordinates. In addition,
these forces are obtained by filtering out the unitary transformations (center-of-
mass translations and rigid rotations for non-periodic systems, see
atom_forces_free_T0_raw for the unfiltered counterpart). Forces due to constraints
such as fixed atoms, distances, angles, dihedrals, etc. are also included (see
atom_forces_free_T0_raw for the unfiltered counterpart)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :atom_forces_type ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_a979c531_f9fa_4a6e_93c1.0.0-alpha2960241ca6> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        :NMI_quantity .

:atom_forces_T0_raw a owl:Class ;
    rdfs:comment """Forces acting on the atoms, calculated as minus gradient of energy_total_T0,
**without** constraints. The derivatives with respect to displacements of the
nuclei are evaluated in Cartesian coordinates. These forces may contain unitary
transformations (center-of-mass translations and rigid rotations for non-periodic
systems) that are normally filtered separately (see atom_forces_T0 for the
filtered counterpart). Forces due to constraints such as fixed atoms, distances,
angles, dihedrals, etc. are also considered separately (see atom_forces_T0 for the
filtered counterpart)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :atom_forces_type ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_a979c531_f9fa_4a6e_93c1.0.0-alpha2960241ca6> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        :NMI_quantity .

:atom_forces_free a owl:Class ;
    rdfs:comment """Forces acting on the atoms, calculated as minus gradient of energy_free,
**including** constraints, if present. The derivatives with respect to
displacements of the nuclei are evaluated in Cartesian coordinates. The
(electronic) energy_free contains the information on the change in (fractional)
occupation of the electronic eigenstates, which are accounted for in the
derivatives, yielding a truly energy-conserved quantity. In addition, these forces
are obtained by filtering out the unitary transformations (center-of-mass
translations and rigid rotations for non-periodic systems, see
atom_forces_free_raw for the unfiltered counterpart). Forces due to constraints
such as fixed atoms, distances, angles, dihedrals, etc. are included (see
atom_forces_free_raw for the unfiltered counterpart)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_a979c531_f9fa_4a6e_93c1.0.0-alpha2960241ca6> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :atom_forces_type ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:atom_forces_free_raw a owl:Class ;
    rdfs:comment """Forces acting on the atoms, calculated as minus gradient of energy_free,
**without** constraints. The derivatives with respect to displacements of nuclei
are evaluated in Cartesian coordinates. The (electronic) energy_free contains the
change in (fractional) occupation of the electronic eigenstates, which are
accounted for in the derivatives, yielding a truly energy-conserved quantity.
These forces may contain unitary transformations (center-of-mass translations and
rigid rotations for non-periodic systems) that are normally filtered separately
(see atom_forces_free for the filtered counterpart). Forces due to constraints
such as fixed atoms, distances, angles, dihedrals, etc. are also considered
separately (see atom_forces_free for the filtered counterpart)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :atom_forces_type ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_a979c531_f9fa_4a6e_93c1.0.0-alpha2960241ca6> ;
