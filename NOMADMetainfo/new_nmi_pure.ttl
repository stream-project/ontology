            owl:hasValue :number_of_temperatures_in_sequence ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:frame_sequence_temperature_stats a owl:Class ;
    rdfs:comment """Average temperature and its standard deviation over this sequence of frames (i.e.,
a trajectory, a frame is one section_single_configuration_calculation)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_2e5e45fc_f52c_4294_bdc2_5ed7a06dfce7> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:frame_sequence_time a owl:Class ;
    rdfs:comment """Time along this sequence of frames (i.e., a trajectory, a frame is one
