    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_lm_atomic_multipoles ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:atomic_multipole_m_kind a owl:Class ;
    rdfs:comment """String describing the definition for each integer number $m$ in
atomic_multipole_lm. Allowed values are listed in the [m_kind wiki
page](https://gitlab.rzg.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/m-kind)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:atomic_multipole_values a owl:Class ;
    rdfs:comment """Value of the multipoles (including the monopole/charge for $l$ = 0, the dipole for
