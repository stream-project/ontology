sequence is a continuation of another time should be continued too."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_frames_in_sequence ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_314ba716_2d3d_4462_9a4f_d3419ae1df43> ;
