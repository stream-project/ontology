            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_normalized_band_segment_eigenvalues ;
            owl:onProperty :hasVarDim3 ],
        :NMI_quantity .

:band_k_points a owl:Class ;
    rdfs:comment """Fractional coordinates of the $k$ or $q$ points (in the basis of the reciprocal-
lattice vectors) for which the electronic energy are given."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_k_points_per_segment ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:band_k_points_normalized a owl:Class ;
    rdfs:comment """Fractional coordinates of the $k$ points (in the basis of the reciprocal-lattice
vectors) for which the normalized electronic energies are given."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_normalized_k_points_per_segment ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:band_occupations a owl:Class ;
    rdfs:comment """Occupation of the $k$-points along the electronic band. The size of the dimensions
of this third-order tensor are the same as for the tensor in band_energies."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_band_segment_eigenvalues ;
            owl:onProperty :hasVarDim3 ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_k_points_per_segment ;
            owl:onProperty :hasVarDim2 ],
        :NMI_quantity .

:band_occupations_normalized a owl:Class ;
    rdfs:comment """Occupation of the $k$-points along the normalized electronic band. The size of the
dimensions of this third-order tensor are the same as for the tensor in
band_energies."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_normalized_k_points_per_segment ;
            owl:onProperty :hasVarDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_normalized_band_segment_eigenvalues ;
            owl:onProperty :hasVarDim3 ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:band_segm_labels a owl:Class ;
    rdfs:comment """Start and end labels of the points in the segment (one-dimensional pathways)
sampled in the $k$-space or $q$-space, using the conventional symbols, e.g.,
Gamma, K, L. The coordinates (fractional, in the reciprocal space) of the start
and end points for each segment are given in band_segm_start_end"""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:band_segm_labels_normalized a owl:Class ;
    rdfs:comment """Start and end labels of the points in the segment (one-dimensional pathways)
sampled in the $k$-space, using the conventional symbols, e.g., Gamma, K, L. The
coordinates (fractional, in the reciprocal space) of the start and end points for
each segment are given in band_segm_start_end_normalized"""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:band_segm_start_end a owl:Class ;
    rdfs:comment """Fractional coordinates of the start and end point (in the basis of the reciprocal
lattice vectors) of the segment sampled in the $k$ space. The conventional symbols
(e.g., Gamma, K, L) of the same points are given in band_segm_labels"""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:band_segm_start_end_normalized a owl:Class ;
    rdfs:comment """Fractional coordinates of the start and end point (in the basis of the reciprocal
lattice vectors) of the segment sampled in the $k$ space. The conventional symbols
(e.g., Gamma, K, L) of the same points are given in band_segm_labels"""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:band_structure_kind a owl:Class ;
    rdfs:comment "String to specify the kind of band structure (either electronic or vibrational)."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:basis_set a owl:Class ;
    rdfs:comment """Unique string identifying the basis set used for the final wavefunctions
calculated with XC_method. It might identify a class of basis sets, often matches
one of the strings given in any of basis_set_name."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_numerical_parameter ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :settings_potential_energy_surface ],
        :NMI_quantity .

:basis_set_atom_centered_ls a owl:Class ;
    rdfs:comment """Azimuthal quantum number ($l$) values (of the angular part given by the spherical
harmonic $Y_{lm}$) of the atom-centered basis function defined in the current
section_basis_set_atom_centered."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_kinds_in_basis_set_atom_centered ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:basis_set_atom_centered_radial_functions a owl:Class ;
    rdfs:comment """Values of the radial function of the different basis function kinds. The values
are numerically tabulated on a default 0.01-nm equally spaced grid from 0 to 4 nm.
The 5 tabulated values are $r$, $f(r)$, $f'(r)$, $f(r) \\cdot r$,
$\\frac{d}{dr}(f(r) \\cdot r)$."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_kinds_in_basis_set_atom_centered ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 401 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 5 ;
            owl:onProperty :hasFixDim3 ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:basis_set_atom_centered_short_name a owl:Class ;
    rdfs:comment """Code-specific, but explicative, base name for the basis set (not unique). Details
are explained in the [basis_set_atom_centered_short_name wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/basis-
set-atom-centered-short-name), this name should not contain the *atom kind* (to
simplify the use of a single name for multiple elements)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:basis_set_atom_centered_unique_name a owl:Class ;
    rdfs:comment """Code-specific, but explicative, base name for the basis set (not unique). This
string starts with basis_set_atom_centered_short_name. If the basis set defined in
this section_basis_set_atom_centered is not identical to the default definition
(stored in a database) of the basis set with the same name stored in a database,
then the string is extended by 10 identifiable characters as explained in the
[basis_set_atom_centered_name wiki page](https://gitlab.mpcdf.mpg.de/nomad-
lab/nomad-meta-info/wikis/metainfo/basis-set-atom-centered-unique-name). The
reason for this procedure is that often atom-centered basis sets are obtained by
fine tuning basis sets provided by the code developers or other sources. Each
basis sets, which has normally a standard name, often reported in publications,
has also several parameters that can be tuned. This metadata tries to keep track
of the original basis set and its modifications. This string here defined should
not contain the *atom kind* for which this basis set is intended for, in order to
simplify the use of a single name for multiple *atom kinds* (see atom_labels for
the actual meaning of *atom kind*)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:basis_set_atom_number a owl:Class ;
    rdfs:comment """Atomic number (i.e., number of protons) of the atom for which this basis set is
constructed (0 means unspecified or a pseudo atom)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        :NMI_dimension,
        :NMI_quantity .

:basis_set_cell_dependent_kind a owl:Class ;
    rdfs:comment """A string defining the type of the cell-dependent basis set (i.e., non atom
centered such as plane-waves). Allowed values are listed in the
[basis_set_cell_dependent_kind wiki page](https://gitlab.mpcdf.mpg.de/nomad-
lab/nomad-meta-info/wikis/metainfo/basis-set-cell-dependent-kind)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:basis_set_cell_dependent_name a owl:Class ;
    rdfs:comment """A label identifying the cell-dependent basis set (i.e., non atom centered such as
plane-waves). Allowed values are listed in the [basis_set_cell_dependent_name wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/basis-
set-cell-dependent-name)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:basis_set_kind a owl:Class ;
    rdfs:comment """String describing the use of the basis set, i.e, if it used for expanding a wave-
function or an electron density. Allowed values are listed in the [basis_set_kind
wiki page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-
info/wikis/metainfo/basis-set-kind)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:basis_set_name a owl:Class ;
    rdfs:comment """String identifying the basis set in an unique way. The rules for building this
string are specified in the [basis_set_name wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/basis-
set-name)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:basis_set_planewave_cutoff a owl:Class ;
    rdfs:comment """Spherical cutoff  in reciprocal space for a plane-wave basis set. It is the energy
of the highest plan-ewave ($\\frac{\\hbar^2|k+G|^2}{2m_e}$) included in the basis
set. Note that normally this basis set is used for the wavefunctions, and the
density would have 4 times the cutoff, but this actually depends on the use of the
basis set by the method."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:bins a owl:Class ;
    rdfs:comment "Byte representation of the DOS fingerprint."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:bravais_lattice a owl:Class ;
    rdfs:comment """Identifier for the Bravais lattice in Pearson notation. The first lowercase letter
identifies the crystal family and can be one of the following: a (triclinic), b
(monoclinic), o (orthorhombic), t (tetragonal), h (hexagonal) or c (cubic). The
second uppercase letter identifies the centring and can be one of the following: P
(primitive), S (face centred), I (body centred), R (rhombohedral centring) or F
(all faces centred)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:calculation_file_uri a owl:Class ;
    rdfs:comment """Contains the nomad uri of a raw the data file connected to the current run. There
should be an value for the main_file_uri and all ancillary files."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:calculation_method a owl:Class ;
    rdfs:comment """String that uniquely represents the method used to calculate energy_total, If the
present calculation_method_current is a perturbative method Y that uses method X
as starting point, this string is automatically created as X@Y, where X is taken
from calculation_method_current and Y from method_to_method_ref. In order to
activate this, method_to_method_kind must have the value starting_point (see the
[method_to_method_kind wiki page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-
meta-info/wikis/metainfo/method-to-method-kind))."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:calculation_method_current a owl:Class ;
    rdfs:comment """String that represents the method used to calculate the energy_current. If the
method is perturbative, this string does not describe the starting point method,
the latter being referenced to by section_method_to_method_refs. For self-
consistent field (SCF) ab initio calculations, for example, this is composed by
concatenating XC_method_current and basis_set. See [calculation_method_current
wiki page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-
info/wikis/metainfo/calculation-method-current) for the details."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:calculation_method_kind a owl:Class ;
    rdfs:comment """Kind of method in calculation_method_current.

Accepted values are:

- absolute

- perturbative."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:calculation_to_calculation_external_url a owl:Class ;
    rdfs:comment """URL used to reference an externally stored calculation. The kind of relationship
between the present and the referenced section_single_configuration_calculation is
specified by calculation_to_calculation_kind."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:calculation_to_calculation_kind a owl:Class ;
    rdfs:comment """String defining the relationship between the referenced
section_single_configuration_calculation and the present
section_single_configuration_calculation. Valid values are described in the
[calculation_to_calculation_kind wiki page](https://gitlab.mpcdf.mpg.de/nomad-
lab/nomad-meta-info/wikis/metainfo/calculation-to-calculation-kind). Often
calculations are connected, for instance, one calculation is a perturbation
performed using a self-consistent field (SCF) calculation as starting point, or a
simulated system is partitioned in regions with different but connected
Hamiltonians (e.g., QM/MM, or a region treated via Kohn-Sham DFT embedded into a
region treated via orbital-free DFT). Hence, the need of keeping track of these
connected calculations. The referenced calculation is identified via
calculation_to_calculation_ref (typically used for a calculation in the same
section_run) or calculation_to_calculation_external_url."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:calculation_to_calculation_ref a owl:Class ;
    rdfs:comment """Reference to another calculation. If both this and
calculation_to_calculation_external_url are given, then
calculation_to_calculation_ref is a local copy of the URL given in
calculation_to_calculation_external_url. The kind of relationship between the
present and the referenced section_single_configuration_calculation is specified
by calculation_to_calculation_kind."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:calculation_to_folder_external_url a owl:Class ;
    rdfs:comment """URL used to reference a folder containing external calculations. The kind of
relationship between the present and the referenced
section_single_configuration_calculation is specified by
calculation_to_folder_kind."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:calculation_to_folder_kind a owl:Class ;
    rdfs:comment """String defining the relationship between the referenced
section_single_configuration_calculation and a folder containing calculations."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:chemical_composition a owl:Class ;
    rdfs:comment "The chemical composition as full formula of the system, based on atom species."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:chemical_composition_bulk_reduced a owl:Class ;
    rdfs:comment """The chemical composition as reduced bulk formula of the system, based on atom
species."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:chemical_composition_reduced a owl:Class ;
    rdfs:comment "The chemical composition as reduced formula of the system, based on atom species."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:choice a owl:Class ;
    rdfs:comment """String that specifies the centering, origin and basis vector settings of the 3D
space group that defines the symmetry group of the simulated physical system (see
section_system). Values are as defined by spglib."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:conduction_band_min_energy a owl:Class ;
    rdfs:comment "Conduction band minimum energy."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:conduction_band_min_k_point a owl:Class ;
    rdfs:comment "Coordinate of the conduction band minimum in k-space."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <https://nomad-coe.eu/ontology/properties#PerMetre> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:configuration_periodic_dimensions a owl:Class ;
    rdfs:comment """Array labeling which of the lattice vectors use periodic boundary conditions. Note
for the parser developers: This value is not expected to be given for each
section_single_configuration_calculation. It is assumed to be valid from the
section_single_configuration_calculation where it is defined for all subsequent
section_single_configuration_calculation in section_run, until redefined."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_54dc83cb_06e1_4739_9e45_bc09cead7f48> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :configuration_core ],
        :NMI_quantity .

:configuration_raw_gid a owl:Class ;
    rdfs:comment """checksum of the configuration_core, i.e. the geometry of the system. The values
are not normalized in any way so equivalent configurations might have different
values"""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:constraint_atoms a owl:Class ;
    rdfs:comment """List of the indexes involved in this constraint. The fist atom has index 1, the
last number_of_topology_atoms."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_constraints ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms_per_constraint ;
            owl:onProperty :hasVarDim2 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:constraint_kind a owl:Class ;
    rdfs:comment """Short and unique name for this constraint type. Valid names are described in the
[constraint\\_kind wiki page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-
info/wikis/metainfo/constraint-kind)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:constraint_parameters a owl:Class ;
    rdfs:comment """Explicit constraint parameters for this kind of constraint (depending on the
constraint type, some might be given implicitly through other means)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:crystal_system a owl:Class ;
    rdfs:comment """Name of the crystal system. Can be one of the following: triclinic, monoclinic,
orthorhombic, tetragonal, trigonal, hexagonal or cubic."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:dft_plus_u_functional a owl:Class ;
    rdfs:comment """Type of DFT+U functional (such as DFT/DFT+U double-counting compensation). Valid
names are described in the [dft\\_plus\\_u\\_functional wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/dft-
plus-u-functional)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:dft_plus_u_orbital_J a owl:Class ;
    rdfs:comment "DFT+U-orbital setting: value J (exchange interaction)"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :energy_value ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:dft_plus_u_orbital_U a owl:Class ;
    rdfs:comment "DFT+U-orbital setting: value U (on-site Coulomb interaction)"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :energy_value ],
        :NMI_quantity .

:dft_plus_u_orbital_U_effective a owl:Class ;
    rdfs:comment "DFT+U-orbital setting: value U_{effective} (U-J), if implementation uses it"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasCategory ;
            owl:someValuesFrom :energy_value ],
        :NMI_quantity .

:dft_plus_u_orbital_atom a owl:Class ;
    rdfs:comment "DFT+U-orbital setting: atom index (references index of atom_labels/atom_positions)"^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        :NMI_dimension,
        :NMI_quantity .

:dft_plus_u_orbital_label a owl:Class ;
    rdfs:comment "DFT+U-orbital setting: orbital label (normally (n,l)), notation: '3d', '4f', ..."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:dft_plus_u_projection_type a owl:Class ;
    rdfs:comment """DFT+U: Type of orbitals used for projection in order to calculate occupation
numbers. Valid names are described in the [dft\\_plus\\_u\\_projection\\_type wiki
page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/dft-
plus-u-projection-type)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:dos_energies a owl:Class ;
    rdfs:comment """Array containing the set of discrete energy values for the density (electronic-
energy or vibrational energy) of states (DOS). This is the total DOS, see
atom_projected_dos_energies and species_projected_dos_energies for partial density
of states."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_dos_values ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:dos_energies_normalized a owl:Class ;
    rdfs:comment """Array containing the set of discrete energy values with respect to the top of the
valence band for the density (electronic-energy) of states (DOS). This is the
total DOS, see atom_projected_dos_energies and species_projected_dos_energies for
partial density of states."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_dos_values ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:dos_fermi_energy a owl:Class ;
    rdfs:comment "Stores the Fermi energy of the density of states."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:dos_integrated_values a owl:Class ;
    rdfs:comment """Integrated density of states (starting at $-\\infty$), pseudo potential
calculations should start with the number of core electrons if they cover only the
active electrons"""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_dos_values ;
            owl:onProperty :hasVarDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:dos_kind a owl:Class ;
    rdfs:comment """String to specify the kind of density of states (either electronic or
vibrational)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:dos_lm a owl:Class ;
    rdfs:comment """Tuples of $l$ and $m$ values for which dos_values_lm are given. For the quantum
number $l$ the conventional meaning of azimuthal quantum number is always adopted.
For the integer number $m$, besides the conventional use as magnetic quantum
number ($l+1$ integer values from $-l$ to $l$), a set of different conventions is
accepted (see the [m_kind wiki page](https://gitlab.rzg.mpg.de/nomad-lab/nomad-
meta-info/wikis/metainfo/m-kind). The actual adopted convention is specified by
dos_m_kind."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_dos_lms ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:dos_m_kind a owl:Class ;
    rdfs:comment """String describing what the integer numbers of $m$ in dos_lm mean. The allowed
values are listed in the [m_kind wiki page](https://gitlab.rzg.mpg.de/nomad-
lab/nomad-meta-info/wikis/metainfo/m-kind)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:dos_values a owl:Class ;
    rdfs:comment """Values (number of states for a given energy, the set of discrete energy values is
given in dos_energies) of density (electronic-energy or vibrational-energy) of
states. This refers to the simulation cell, i.e. integrating over all energies
will give the number of electrons in the simulation cell."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_dos_values ;
            owl:onProperty :hasVarDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:dos_values_lm a owl:Class ;
    rdfs:comment """Array containing the density (electronic-energy) of states values projected on the
various spherical harmonics (integrated on all atoms), see
atom_projected_dos_values_lm for atom values."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 4 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim3 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim2 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_dos_values ;
            owl:onProperty :hasVarDim4 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_dos_lms ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        :NMI_quantity .

:dos_values_normalized a owl:Class ;
    rdfs:comment """Density of states (DOS) values divided by the unit cell volume and by the number
of atoms."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_spin_channels ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_dos_values ;
            owl:onProperty :hasVarDim2 ],
        :NMI_quantity .

:eigenvalues_kind a owl:Class ;
    rdfs:comment """A short string describing the kind of eigenvalues, as defined in the
[eigenvalues_kind wiki page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-
info/wikis/metainfo/eigenvalues-kind)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:eigenvalues_kpoints a owl:Class ;
    rdfs:comment """Coordinates of the $k$ points (in the basis of the reciprocal lattice vectors)
used for the evaluation of the eigenvalues tabulated in eigenvalues_values."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_eigenvalues_kpoints ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
