        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_atoms ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:atomic_multipole_kind a owl:Class ;
    rdfs:comment """String describing the method used to obtain the electrostatic multipoles
(including the electric charge, dipole, etc.) for each atom. Such multipoles
require a charge-density partitioning scheme, specified by the value of this
