    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_topology_atoms ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:atom_type_charge a owl:Class ;
    rdfs:comment "Charge of the atom type."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_696ed548_9477_45ea_993c_6a8f5271914a> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:atom_type_mass a owl:Class ;
    rdfs:comment "Mass of the atom type."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_9bfd6f1e_b0ce_459c_beb7_8f1f41708bba> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:atom_type_name a owl:Class ;
    rdfs:comment "Name (label) of the atom type."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:atom_velocities a owl:Class ;
    rdfs:comment """Velocities of the nuclei, defined as the change in Cartesian coordinates of the
nuclei with respect to time."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
