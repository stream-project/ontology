    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

:frame_sequence_user_quantity a owl:Class ;
    rdfs:comment """Array containing the values of the user-defined quantity defined in
frame_sequence_user_quantity_name, evaluated along this sequence of frames (i.e.,
trajectory, a frame is one section_single_configuration_calculation). If not all
frames have a value the indices of the frames that have a value are stored in
frame_sequence_kinetic_energy_frames. If not all frames have a value the indices
of the frames that have a value are stored in
frame_sequence_kinetic_energy_frames."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_frame_sequence_user_quantity_components ;
            owl:onProperty :hasVarDim2 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_user_quantity_evaluations_in_sequence ;
            owl:onProperty :hasVarDim1 ],
        :NMI_quantity .

:frame_sequence_user_quantity_frames a owl:Class ;
    rdfs:comment """Array containing the strictly increasing indices referring to the frames of
frame_sequence_user_quantity. If not given it defaults to the trivial mapping
0,1,..."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue :number_of_user_quantity_evaluations_in_sequence ;
            owl:onProperty :hasVarDim1 ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_f8bd64d5_5d3e_4ad4_a46e_c30714fecb7f> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:frame_sequence_user_quantity_name a owl:Class ;
    rdfs:comment """Descriptive name of a user-defined quantity, sampled along this sequence of frames
(i.e., a trajectory, a frame is one section_single_configuration_calculation).
Dedicated metadata are created for the conserved energy-like quantity
(frame_sequence_conserved_quantity), the kinetic and potential energies
(frame_sequence_kinetic_energy and frame_sequence_potential_energy), the
instantaneous temperature (frame_sequence_temperature) and pressure
(frame_sequence_pressure). This metadata should be used for other quantities that
are monitored along a sequence of frames."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_quantity .

:frame_sequence_user_quantity_stats a owl:Class ;
    rdfs:comment """Average of frame_sequence_user_quantity and its standard deviation in this
sequence of frames (i.e., a trajectory, a frame is one
section_single_configuration_calculation)."""^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasFixDim1 ],
        [ a owl:Restriction ;
            owl:hasValue :number_of_frame_sequence_user_quantity_components ;
            owl:onProperty :hasVarDim2 ],
        [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_quantity .

