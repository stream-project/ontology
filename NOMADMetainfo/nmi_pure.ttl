            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onClass :number_of_atoms_in_molecule ;
            owl:onProperty :hasVarDim1 ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_696ed548_9477_45ea_993c_6a8f5271914a> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :settings_atom_in_molecule ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_concrete .

:atom_in_molecule_name a owl:Class ;
    rdfs:comment "Name (label) of each atom in the molecule."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onClass :number_of_atoms_in_molecule ;
            owl:onProperty :hasVarDim1 ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :settings_atom_in_molecule ],
        :NMI_concrete .

:atom_labels a owl:Class ;
    rdfs:comment "Labels of the atoms. These strings identify the atom kind and conventionally start with the symbol of the atomic species, possibly followed by the atomic number. The same atomic species can be labeled with more than one atom_labels in order to distinguish, e.g., atoms of the same species assigned to different atom-centered basis sets or pseudo-potentials, or simply atoms in different locations in the structure (e.g., bulk and surface). These labels can also be used for *particles* that do not correspond to physical atoms (e.g., ghost atoms in some codes using atom-centered basis sets). This metadata defines a configuration and is therefore required."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :configuration_core ],
        [ a owl:Restriction ;
            owl:onClass :number_of_atoms ;
            owl:onProperty :hasVarDim1 ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_concrete .

:atom_positions a owl:Class ;
    rdfs:comment "Positions of all the atoms, in Cartesian coordinates. This metadata defines a configuration and is therefore required. For alloys where concentrations of species are given for each site in the unit cell, it stores the position of the sites."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 2 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim2 ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :configuration_core ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_7db11dbf_a643_464a_9b56_07eabcc3e9c5> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:onClass :number_of_atoms ;
            owl:onProperty :hasVarDim1 ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        :NMI_concrete .

:basis_set a owl:Class ;
    rdfs:comment "Unique string identifying the basis set used for the final wavefunctions calculated with XC_method. It might identify a class of basis sets, often matches one of the strings given in any of basis_set_name."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :settings_potential_energy_surface ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :settings_numerical_parameter ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        :NMI_concrete .

:calculation_context a owl:Class ;
    rdfs:comment "Contains information relating to a calculation."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasConcreteValue ;
            owl:someValuesFrom :calculation_gid ],
        :NMI_section .

:configuration_periodic_dimensions a owl:Class ;
    rdfs:comment "Array labeling which of the lattice vectors use periodic boundary conditions. Note for the parser developers: This value is not expected to be given for each section_single_configuration_calculation. It is assumed to be valid from the section_single_configuration_calculation where it is defined for all subsequent section_single_configuration_calculation in section_run, until redefined."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :configuration_core ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_54dc83cb_06e1_4739_9e45_bc09cead7f48> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 1 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:hasValue 3 ;
            owl:onProperty :hasFixDim1 ],
        :NMI_concrete .

:conserved_quantity a owl:Class ;
    rdfs:comment "A quantity that is preserved during the time propagation (for example, kinetic+potential energy during NVE)."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_abstract .

:electronic_structure_method a owl:Class ;
    rdfs:comment "Non-unique string identifying the used electronic structure method. It is not unique in the sense that two calculations with the same electronic_structure_method string may have not been performed with exactly the same method. The allowed strings are given in the [electronic structure method wiki page](https://gitlab.mpcdf.mpg.de/nomad-lab/nomad-meta-info/wikis/metainfo/electronic-structure-method)."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/perceptual#EMMO_50ea1ec5_f157_41b0_b46b_a9032f17ca10> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :settings_XC ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_concrete .

:embedded_system a owl:Class ;
    rdfs:comment "Is the system embedded into a host geometry?."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :configuration_core ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_54dc83cb_06e1_4739_9e45_bc09cead7f48> ;
            owl:onProperty :hasDatatype ],
        :NMI_concrete .

:energy_C a owl:Class ;
    rdfs:comment "Correlation (C) energy calculated with the method described in XC_functional."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :energy_type_C ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        :NMI_concrete .

:energy_C_mGGA a owl:Class ;
    rdfs:comment "Component of the correlation (C) energy at the GGA (or MetaGGA) level using the self-consistent density of the target XC functional (full unscaled value, i.e., not scaled due to exact-exchange mixing)."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasValue 0 ;
            owl:onProperty :hasNumberOfDimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/siunits#EMMO_8a70dea4_d6ab_4260_b931_a3e990982416> ;
            owl:onProperty <http://emmo.info/emmo/middle/metrology#EMMO_67fc0a36_8dcb_4ffa_9a43_31074efa3296> ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
            owl:someValuesFrom :energy_type_C ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        :NMI_concrete .

:energy_X a owl:Class ;
    rdfs:comment "Value fo the exchange (X) energy calculated with the method described in XC_method."^^xsd:string ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://emmo.info/emmo/middle/math#EMMO_18d180e4_5e3e_42f7_820c_e08951223486> ;
            owl:onProperty :hasDatatype ],
        [ a owl:Restriction ;
            owl:onProperty :hasAbstractType ;
